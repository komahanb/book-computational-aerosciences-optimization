@article{BauchauBenchmark2016,
abstract = {As the need to model flexibility arose in multibody dynamics, the floating frame of reference formulation was developed, but this approach can yield inaccurate results when elastic displacements becomes large. While the use of three-dimensional finite element formulations overcomes this problem, the associated computational cost is overwhelming. Consequently, beam models, which are one-dimensional approximations of three-dimensional elasticity, have become the workhorse of many flexible multibody dynamics codes. Numerous beam formulations have been proposed, such as the geometrically exact beam formulation or the absolute nodal coordinate formulation, to name just two. New solution strategies have been investigated as well, including the intrinsic beam formulation or the DAE approach. This paper provides a systematic comparison of these various approaches, which will be assessed by comparing their predictions for four benchmark problems. The first problem is the Princeton beam experiment, a study of the static large displacement and rotation behavior of a simple cantilevered beam under a gravity tip load. The second problem, the four-bar mechanism, focuses on a flexible mechanism involving beams and revolute joints. The third problem investigates the behavior of a beam bent in its plane of greatest flexural rigidity, resulting in lateral buckling when a critical value of the transverse load is reached. The last problem investigates the dynamic stability of a rotating shaft. The predictions of eight independent codes are compared for these four benchmark problems and are found to be in close agreement with each other and with experimental measurements, when available.},
author = {Bauchau, Olivier A and Betsch, Peter and Cardona, Alberto and Gerstmayr, Johannes and Jonker, Ben and Masarati, Pierangelo and Sonneville, Valentin},
doi = {10.1007/s11044-016-9514-y},
issn = {1573-272X},
journal = {Multibody System Dynamics},
month = {may},
number = {1},
pages = {29--48},
title = {{Validation of flexible multibody dynamics beam formulations using benchmark problems}},
url = {https://doi.org/10.1007/s11044-016-9514-y},
volume = {37},
year = {2016}
}


@inbook{Haug1998,
abstract = {Implicit integration approaches based on generalized coordinate-partitioning of the differential-algebraic equations of motion of multibody dynamics are presented. The methods developed are intended for simulation of stiff mechanical systems using the well-known Newmark integration method from structural dynamics and more recent implicit Runge-Kutta methods. Newmark integration formulas for second-order differential equations are first used to define independent generalized coordinates and their first time-derivative as functions of independent accelerations. The latter are determined as the solution of discretized equations obtained using the descriptor form of the equations of motion. Dependent variables in the formulation, including Lagrange multipliers, are determined to satisfy all the kinematic and kinetic equations of multibody dynamics. Next, the process is extended to implicit Runge-Kutta methods with variable step size and error control. The approach is illustrated by solving the constrained equations of motion for mechanical systems that exhibit stiff behavior. Results show that the approach is robust and has the capability to integrate differential-algebraic equations of motion for stiff multibody dynamic systems},
address = {Berlin, Heidelberg},
author = {Haug, E J and Negrut, D and Iancu, M},
booktitle = {Computational Methods in Mechanical Systems: Mechanism Analysis, Synthesis, and Optimization},
doi = {10.1007/978-3-662-03729-4_11},
editor = {Angeles, Jorge and Zakhariev, Evtim},
isbn = {978-3-662-03729-4},
pages = {242--267},
publisher = {Springer Berlin Heidelberg},
title = {{Implicit Integration of the Equations of Multibody Dynamics}},
url = {http://dx.doi.org/10.1007/978-3-662-03729-4\_11},
year = {1998}
}


