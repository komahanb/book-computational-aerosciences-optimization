@inbook{2004-Miguel-Krenk,
author = {Gutiérrez, Miguel A. and Krenk, Steen},
publisher = {John Wiley \& Sons},
isbn = {9780470091357},
title = {Stochastic Finite Element Methods},
booktitle = {Encyclopedia of Computational Mechanics},
chapter = {20},
pages = {},
doi = {10.1002/0470091355.ecm044},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/0470091355.ecm044},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/0470091355.ecm044},
year = {2004},
keywords = {stochastic finite elements, random field discretization, parameter sensitivity, perturbation method, generalized Hermite polynomials, polynomial chaos expansion, reliability methods},
abstract = {Abstract This chapter gives an overview of finite element related methods for the solution of problems in which the system has random properties. The notion of stochastic variables and fields is introduced and details are given for the normal and log-normal distributions. Discretization methods for random fields and efficient algorithms for evaluation of the sensitivity to the random input are presented. The two dominating approximate methods for obtaining the statistical properties of the response from those of the stochastic properties of the system – the perturbation and the spectral method – are described in detail, and it is explained how statically determinate systems constitute a special case, permitting more general results. The final section deals with reliability methods, the purpose of which is to obtain the probability or a probability estimate, of a rare event, for example, failure. The methods are introduced by means of simple examples, with a view to making the text a self-contained introduction and survey.}
}


